{
  "name": "atf-5320-23-generator",
  "version": "1.0.0",
  "description": "NFA Form processor with mupdf.js for client-side PDF generation",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack serve --mode=development",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "pdf",
    "forms",
    "nfa",
    "mupdf"
  ],
  "author": "Chaz Schlarp",
  "repository": "github:schlarpc/atf-5320.23-generator",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "mupdf": "1.26.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "copy-webpack-plugin": "^13.0.1",
    "html-webpack-plugin": "5.6.5",
    "prettier": "^3.7.4",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "webpack": "5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.0.0"
  }
}
